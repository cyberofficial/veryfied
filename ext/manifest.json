{
	"manifest_version": 3,
	"name": "Veryfied",
	"version": "1.0",
	"author": "Simone Margaritelli ( @evilsocket )",
	"description": "Mark pre-Musk era Twitter verified accounts.",
	"content_scripts": [
		{
			"matches": [
				"*://*.twitter.com/*"
			],
			"js": [
				"db.js",
				"veryfied.js"
			],
			"run_at": "document_start"
		}
	],
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}